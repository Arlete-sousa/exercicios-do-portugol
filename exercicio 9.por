programa
{
	inteiro contador
	funcao inicio()
	{
		escreva("Contagem crescente de 101 a 110: ")//contagem crescente do 101 até o 110

		para(contador = 101; contador <=109; contador ++){
			escreva(contador, ", ")
		}
		escreva("110.")
	}
}
